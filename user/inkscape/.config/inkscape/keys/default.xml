<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="User Shortcuts">
  <bind
     gaction="win.tool-switch('Pencil')"
     keys="&lt;Shift&gt;p" />
  <bind
     gaction="win.tool-switch('Pen')"
     keys="p" />
  <bind
     gaction="win.tool-switch('Node')"
     keys="v" />
  <bind
     gaction="win.tool-toggle('Select')"
     keys="a" />
  <bind
     gaction="app.paste-size"
     keys="&lt;Primary&gt;i" />
</keys>
